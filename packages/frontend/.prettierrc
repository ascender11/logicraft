{
  "printWidth": 100,
  "singleQuote": true,
  "trailingComma": "all",
  "semi": true,
  "tabWidth": 2,
  "bracketSpacing": true,
  "arrowParens": "always",
  "useTabs": false,
  "jsxSingleQuote": true,
  "bracketSameLine": true,
  "proseWrap": "preserve",
  "singleAttributePerLine": true,
  "plugins": ["prettier-plugin-tailwindcss", "prettier-plugin-classnames", "prettier-plugin-merge"]
}
